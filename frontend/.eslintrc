{
  "extends": "standard",
  "parser": "babel-eslint",
  "plugins": ["react"],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "react/jsx-uses-vars": 1,
    "camelcase": 0
  },
  "env": {
    "node": false,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "process": 1,
    "module": 1
  }
}